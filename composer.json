{
    "name": "brokunMusheen/atomic-cms",
    "description": "A lightweight CMS component built for Laravel.",
    "keywords": ["cms", "laravel"],
    "license": "MIT",
    "type": "project",
    "support": {
        "issues": "https://github.com/brokunMusheen/atomic-cms/issues",
        "source": "https://github.com/brokunMusheen/atomic-cms"
    },
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "BrokunMusheen\\Atomic\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
